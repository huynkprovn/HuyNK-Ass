	GUICtrlSetFont(-1, 13, 400, 0, "Arial")
	GUICtrlSetOnEvent(-1, "Crack")
	GUICtrlCreateButton("ÄÄng xuáº¥t", 58, 96, 185, 41)
	GUICtrlSetFont(-1, 13, 400, 0, "Arial")
	GUICtrlSetOnEvent(-1, "Logout")
	GUISetState(@SW_SHOW)
