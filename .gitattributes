	GUISetOnEvent($gui_event_close, "Logout")
	Local $hello = GUICtrlCreateLabel("ChÃ o " & $user, 10, 15, 280, 23, $es_center)
	GUICtrlSetFont(-1, 13, 400, 0, "Arial")
