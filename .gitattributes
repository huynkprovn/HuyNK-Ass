	If IsArray($api_isonline) AND $api_isonline[0] = 1 Then
		Local $ans = MsgBox(4 + 32 + 262144, "ÄÄng nháº­p", "TÃ i khoáº£n Äang online, báº¡n cÃ³ muá»n ÄÃ¡ tÃ i khoáº£n?")
		If $ans = 6 Then login2()
	Else
		login2()
	EndIf
EndFunc

